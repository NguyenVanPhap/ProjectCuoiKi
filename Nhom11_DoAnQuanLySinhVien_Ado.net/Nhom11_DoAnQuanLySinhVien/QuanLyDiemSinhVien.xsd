                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT KQHT.MASV,SV.TenSV,KH.TenKhoa,lp.TenLop,ROUND(SUM(KQHT.DiemTongKet*mn.SoTinChi)/SUM(mn.SoTinChi),2) AS 'TungBinh' 
FROM dbo.KETQUAHOCTAP AS KQHT 
INNER JOIN dbo.SINHVIEN AS SV ON SV.MaSv = KQHT.MASV 
