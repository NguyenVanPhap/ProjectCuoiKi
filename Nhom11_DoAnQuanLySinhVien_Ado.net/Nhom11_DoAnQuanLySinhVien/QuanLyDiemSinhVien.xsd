INNER JOIN dbo.MonHoc AS mn ON mn.MaMH = KQHT.MaMH
WHERE KH.MaKhoa=@khoa
GROUP BY KQHT.MASV,SV.TenSV,KH.TenKhoa,lp.TenLop</CommandText>
